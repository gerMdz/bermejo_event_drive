{
  "name": "germdz/edasf7",
  "description": "Curso EDASf7",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "GerMdz",
      "email": "gerardo.montivero@gmail.com"
    }
  ],
  "type": "project",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "7.1.2",
    "symfony/dotenv": "7.1.1",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.1.2",
    "symfony/http-client": "7.1.2",
    "symfony/property-access": "7.1.1",
    "symfony/runtime": "7.1.1",
    "symfony/serializer": "7.1.2",
    "symfony/validator": "7.1.2",
    "symfony/yaml": "7.1.1"
  },
  "require-dev": {
    "overtrue/phplint": "9.4.1",
    "phpstan/phpstan": "1.11.7",
    "phpunit/phpunit": "11.2.7",
    "squizlabs/php_codesniffer": "3.10.1",
    "symfony/browser-kit": "7.1.1",
    "symfony/stopwatch": "7.1.*",
    "symfony/web-profiler-bundle": "7.1.*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "run-tests": "composer run-phpunit && composer run-phpstan && composer run-phpcs && composer run-phplint",
    "run-phpstan": "vendor/bin/phpstan analyse",
    "run-phpcs": "vendor/bin/phpcs",
    "run-phplint": "vendor/bin/phplint",
    "run-phpunit": "vendor/bin/phpunit --testsuite Unit,Application --testdox"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.1.*"
    }
  }
}
